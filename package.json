{
    "name": "nosft-core",
    "version": "2.3.85",
    "private": false,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "author": "deezy",
    "engines": {
        "node": ">=18.0.0"
    },
    "description": "Tools for making a Nosft client.",
    "repository": {
        "type": "git",
        "url": "https://github.com/deezy-inc/nosft-core"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts-resolve",
        "dev": "tsup src/index.ts --format esm,cjs --watch --dts-resolve",
        "lint": "eslint \"src/**/*.ts*\"",
        "clean": "rm -rf node_modules && rm -rf dist",
        "watch": "chokidar 'src/**/*.ts' -c 'npm run build'"
    },
    "files": [
        "dist"
    ],
    "license": "Public domain",
    "dependencies": {
        "@noble/hashes": "^1.3.0",
        "@scure/base": "1.1.1",
        "@scure/btc-signer": "0.5.1",
        "@stacks/common": "^6.0.0",
        "axios": "^1.2.6",
        "bip32": "^3.1.0",
        "bitcoin-address-validation": "^2.2.1",
        "bitcoinjs-lib": "github:deezy-inc/bitcoinjs-lib",
        "clsx": "^1.2.1",
        "ecpair": "^2.1.0",
        "ethers": "^5.7.2",
        "match-sorter": "^6.3.1",
        "nostr-tools": "1.10.1",
        "sats-connect": "0.2.0",
        "tiny-secp256k1": "^2.2.1",
        "varuint-bitcoin": "^1.1.2"
    },
    "resolutions": {
        "tiny-secp256k1": "^2.2.1"
    },
    "keywords": [],
    "devDependencies": {
        "@esbuild-plugins/node-globals-polyfill": "0.2.3",
        "@esbuild-plugins/node-modules-polyfill": "0.2.2",
        "@types/node": "^18.13.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "chokidar-cli": "3.0.0",
        "esbuild": "0.16.9",
        "esbuild-plugin-alias": "^0.2.1",
        "eslint": "^8.33.0",
        "eslint-plugin-babel": "^5.3.1",
        "esm-loader-typescript": "^1.0.3",
        "events": "^3.3.0",
        "jest": "^29.4.2",
        "node-fetch": "^2.6.9",
        "prettier": "^2.8.6",
        "ts-jest": "^29.0.5",
        "tsd": "^0.22.0",
        "typescript": "^4.9.5",
        "websocket-polyfill": "^0.0.3",
        "tsup": "^6.7.0"
    }
}
